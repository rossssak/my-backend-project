{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/nurses",
        "dest": "api/index.js"
      },
      {
        "src": "/register",
        "dest": "api/index.js"
      },
      {
        "src": "/login",
        "dest": "api/index.js"
      },
      {
        "src": "/slist(/?.*)",
        "dest": "api/index.js"
      },
      {
        "src": "/api/(.*)",
        "dest": "api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }